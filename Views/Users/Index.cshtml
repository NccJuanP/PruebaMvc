<a asp-action="Create" class="btn btn-secondary">Create User</a>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Names</th>
      <th scope="col">Last Names</th>
      <th scope="col">Email</th>
      <th scope="col">Acctions</th>
    </tr>
  </thead>
  <tbody>
    @foreach(var user in Model){
    <tr>
      <th scope="row">@user.Id</th>
      <td>@user.Names</td>
      <td>@user.LastNames</td>
      <td>@user.Email</td>
      <td>
        <a class="btn btn-info" asp-action="Detaills" asp-route-id="@user.Id">Details</a>
        <a class="btn btn-success">Update</a>
        <a class="btn btn-danger">Delete</a>
      </td>
    </tr>
    }
  </tbody>
</table>